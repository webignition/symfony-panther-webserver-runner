{
    "name": "webignition/symfony-panther-webserver-runner",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/symfony-panther-webserver-runner",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\SymfonyPantherWebServerRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\SymfonyPantherWebServerRunner\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2",
        "symfony/panther": "^0.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "3.*"
    }
}
